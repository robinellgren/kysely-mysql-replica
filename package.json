{
  "name": "kysely-replica-dialect",
  "author": "robinellgren",
  "version": "0.3.0",
  "description": "Kysely dialect for MySQL and Postgres with support for replicas",
  "license": "MIT",
  "keywords": [
    "kysely",
    "replica",
    "typescript",
    "mysql",
    "postgres",
    "pg",
    "dialect"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . && prettier --check .",
    "build": "tsup ./src/index.ts",
    "lint:fix": "eslint . --fix ",
    "prettier:fix": "prettier --write .",
    "test": "vitest run --coverage .",
    "check-exports": "attw --pack .",
    "release": "pnpm run build && changeset publish",
    "changeset": "changeset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinellgren/kysely-replica-dialect.git"
  },
  "bugs": {
    "url": "https://github.com/robinellgren/kysely-replica-dialect/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@testcontainers/mysql": "^10.18.0",
    "@tsconfig/node20": "^20.0.0",
    "@types/node": "^20.14.8",
    "@vitest/coverage-v8": "3.0.5",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "@types/eslint-config-prettier": "6.11.3",
    "eslint-plugin-perfectionist": "^4.9.0",
    "mysql2": "^3.12.0",
    "prettier": "^3.3.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.5",
    "@arethetypeswrong/cli": "0.17.3",
    "jiti": "2.4.2",
    "@changesets/cli": "2.27.12",
    "@changesets/changelog-github": "0.5.0",
    "kysely": "^0.27.5"
  },
  "peerDependencies": {
    "kysely": "*"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.4.0",
  "volta": {
    "pnpm": "10.4.0",
    "node": "20.18.3"
  }
}