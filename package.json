{
  "name": "kysely-mysql-replica",
  "author": "robinellgren",
  "version": "0.0.2",
  "description": "Kysely dialect for MySQL with support for replicas",
  "license": "MIT",
  "keywords": [
    "kysely",
    "replica",
    "typescript",
    "mysql",
    "dialect"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . && prettier --check .",
    "build": "tsup ./src/index.ts",
    "lint:fix": "eslint . --fix ",
    "prettier:fix": "prettier --write .",
    "tsc": "tsc",
    "test": "vitest run --coverage ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinellgren/kysely-mysql-replica.git"
  },
  "bugs": {
    "url": "https://github.com/robinellgren/kysely-mysql-replica/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@testcontainers/mysql": "^10.18.0",
    "@tsconfig/node20": "^20.0.0",
    "@types/node": "^20.14.8",
    "@vitest/coverage-v8": "3.0.5",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-perfectionist": "^4.9.0",
    "mysql2": "^3.12.0",
    "prettier": "^3.3.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "kysely": "^0.27.5"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.4.0",
  "volta": {
    "pnpm": "10.4.0",
    "node": "20.18.3"
  }
}